{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "rootDir": "./src",
    "lib": ["ESNext", "DOM", "DOM.AsyncIterable", "DOM.Iterable"],
    "outDir": "dist",
    "esModuleInterop": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "src/runtime.ts",
    "src/hydrate-me.ts",
    "src/client/jsx-runtime.ts"
  ]
}
