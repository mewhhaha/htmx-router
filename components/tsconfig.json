{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "outDir": "dist",
    "lib": ["ESNext", "DOM", "DOM.AsyncIterable"],
    "jsx": "react-jsx",
    "jsxImportSource": "runtime/client",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
